{
  "name": "vanguard-security-os",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron/main.cjs",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron:dev": "concurrently \"npm:dev\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:build": "npm run build && electron .",
    "electron:rebuild": "electron-rebuild -f -w better-sqlite3",
    "postinstall": "electron-rebuild -f -w better-sqlite3"
  },
  "dependencies": {
    "@mdxeditor/editor": "^3.0.0",
    "docx": "^9.0.3",
    "html-to-docx": "^1.8.0",
    "nodemailer": "^6.10.0",
    "react-dom": "^19.2.4",
    "react": "^19.2.4",
    "react-router-dom": "^7.13.0",
    "lucide-react": "^0.563.0",
    "recharts": "^3.7.0",
    "better-sqlite3": "^11.8.1"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "concurrently": "^9.0.0",
    "electron": "^26.1.0",
    "electron-rebuild": "^3.2.9",
    "wait-on": "^7.0.1"
  }
}
